@InProceedings{KaiAlexChris04,
  author = {Kai-Florian Richter and Alexander Klippel and Christian Freksa},
  title = {Shortest, Fastest, - but what Next? A Different Approach to Route Directions},
  year = {2004},
  editor = {Martin Raubal and Adam Sliwinski and Werner Kuhn},
  booktitle = {Geoinformation und Mobilit{\"a}t - von der Forschung zur praktischen Anwendung. Beitr{\"a}ge zu den M{\"u}nsteraner GI-Tagen 2004},
  publisher = {Institut f{\"u}r Geoinformatik; M{\"u}nster},
  series = {IfGIprints},
  pages = {205-217},
  pdfurl = {http://www.cosy.informatik.uni-bremen.de/staff/richter/pubs/richter_klippel_freksa.pdf},
  abstract = {Current wayfinding assistance systems calculate primarily two kinds of routes: the shortest or the fastest. To overcome this limitation, recent research extends options in route choice. All approaches, however, provide the same kind of information independent of the route at hand, i.e. all route directions are created the same way irrespective of the route's characteristics. In contrast, we will frame a model that allows for assigning route directions according to route characteristics by developing a systematics for a formal treatment and the automatic generation of route-specific route directions. In our analysis, we concentrate on decision point / action pairs; the systematics considers the structure of an environment and its elements that can be used in giving route directions. We introduce the definition of superordinate route structures and sketch the optimization process used to create route-specific route directions.},
  status = {Reviewed}
}

@InProceedings{KaiAlex04,
  author = {Kai-Florian Richter and Alexander Klippel},
  title = {A Model for Context-Specific Route Directions},
  year = {2005},
  editor = {Christian Freksa and Markus Knauff and Bernd Krieg-Br{\"u}ckner and Bernhard Nebel and Thomas Barkowsky},
  booktitle = {Spatial Cognition IV. Reasoning, Action, Interaction: International Conference Spatial Cognition 2004},
  publisher = {Springer; Berlin},
  pages = {58-78},
  keywords = {context-specific route directions wayfinding assistance  conceptualization},
  url = {http://www.springerlink.com/app/home/issue.asp?wasp=4h83eykqlj0vywe0eaak&referrer=backto&backto=journal,60,1931;linkingpublicationresults,1:105633,1;&absoluteposition=4#A4},
  pdfurl = {http://www.cosy.informatik.uni-bremen.de/staff/richter/pubs/richter2005-csrd.pdf},
  abstract = {Wayfinding, i.e. getting from some origin to a destination, is one of the prime everyday problems humans encounter. It has received a lot of attention in research and many (commercial) systems propose assistance in this task. We present an approach to route directions based on the idea to adapt route directions to route and environment's characteristics. The lack of such an adaptation is a major drawback of existing systems. Our approach is based on an information- and representation-theoretic analysis of routes and takes into account findings of behavioral research. The resulting systematics is the framework for the optimization process. We discuss the consequences of using an optimization process for generating route directions and outline its algorithmic realization.},
  status = {Reviewed}
}



